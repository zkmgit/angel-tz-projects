.car-container {
  background-color: #F4F5F9;
  height: 100vh;
  font-size: 26rpx;
}
.car-container .manager {
  margin-left: 85vw;
  padding: 15rpx 0;
  font-size: 32rpx;
  color: #6E6E6E;
}
.car-container .carList {
  padding: 0 20rpx;
  background-color: #FFFFFF;
}
.car-container .carList .car-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 2rpx solid #EEEEEE;
  background-color: #FFFFFF;
  padding: 20rpx 0;
}
.car-container .carList .car-item .radio {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-right: 20rpx;
}
.car-container .carList .car-item .img {
  width: 120rpx;
  height: 120rpx;
}
.car-container .carList .car-item .img image {
  width: 100%;
  height: 100%;
}
.car-container .carList .car-item .info {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20rpx;
}
.car-container .carList .car-item .info .title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.car-container .carList .car-item .info .price {
  margin-top: 20rpx;
  color: red;
  font-size: 24rpx;
}
.car-container .carList .car-item .stepper {
  margin-top: 10%;
}
.car-container .bottomFiexd {
  position: fixed;
  bottom: 0;
  width: 100vw;
}
.car-container .footer {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx;
  background-color: #FFFFFF;
}
.car-container .footer .right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.car-container .footer .right .price {
  margin-right: 15rpx;
  color: #FF7900;
}
.car-container .footer .right .price .fs {
  color: #6E6E6E;
  font-size: 32rpx;
}
.car-container .footer .right .btn {
  background-color: #FF0000;
  color: #FFFFFF;
  width: 200rpx;
  height: 80rpx;
  border-radius: 50rpx 50rpx 50rpx 50rpx;
  text-align: center;
  line-height: 80rpx;
}
.car-container .footer .delBtn {
  background-color: #FF0000;
  color: #FFFFFF;
  width: 200rpx;
  height: 80rpx;
  border-radius: 50rpx 50rpx 50rpx 50rpx;
  text-align: center;
  line-height: 80rpx;
}

